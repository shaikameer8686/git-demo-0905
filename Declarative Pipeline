pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                echo " I am in a Build Stage"
            }
        }
        stage('Test') { 
            steps {
                echo "I am in Test Stage"
            }
        }
        stage('Deploy') { 
            steps {
                echo "I am in Deploy stage"
            }
        }
    }
}
